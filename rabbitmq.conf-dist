<LoadPlugin python>
    Globals true
</LoadPlugin>

<Plugin python>
    ModulePath "/etc/collectd/scripts/rabbitmq-collectd-plugin/"
    LogTraces true
    Import "rabbitmq_info"
    <Module rabbitmq_info>
       Vhost "/"
       Api "http://localhost:15672"
       UserPass "guest:guest"
       #RmqcBin
       #PmapBin
       #PidFile
       #Verbose boolean
       #Vhost string
    </Module>
</Plugin>

